config/icon="res://BGS-Godot-LogoTemp.svg"

[editor_plugins]

enabled=PackedStringArray("res://addons/BGS-Godot/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
